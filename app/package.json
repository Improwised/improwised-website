{
  "name": "improwised-technologies",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore . --fix",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:style",
    "test": "jest",
    "webpack:build": "webpack --config webpack.config.js",
    "webpack:watch": "webpack --watch"
  },
  "lint-staged": {
    "*.{js,vue,css}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/sitemap": "^2.4.0",
    "core-js": "^3.23.5",
    "granim": "^2.0.0",
    "http-serve": "^1.0.1",
    "moment": "^2.29.3",
    "nuxt": "^2.15.7",
    "nuxt-lazysizes": "^1.4.1",
    "vue-smooth-scroll": "^1.0.13",
    "vue-tinybox": "^1.3.0",
    "vue-typed-js": "^0.1.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.2",
    "compression-webpack-plugin": "^6.1.1",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.0.5",
    "lint-staged": "^12.1.2",
    "mini-css-extract-plugin": "^0.8.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "vue-jest": "^3.0.4",
    "webpack-cli": "^4.9.1"
  }
}
